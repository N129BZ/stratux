{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch rPI/x86",
            "type": "go",
            "request": "launch",
            "mode": "exec",
            "cwd": "${workspaceRoot}",
            "program": "${workspaceRoot}/gen_gdl90",
            "env": {"CGO_CFLAGS_ALLOW": "-L${workspaceRoot}"},
            "args": ["-log_dir", "/tmp/", "-logtostderr"],
            "preLaunchTask": "PreStart",
            "internalConsoleOptions": "openOnSessionStart"
        }
    ]
}